  getmateriAPi() {
    fetch('https://berbagipendidikan.org/sim/api/materi/getmateri')
      .then(res => {
        if (res.status === 200) return res.json();
      })
      .then(resdata => {
        console.log(resdata.DATA);
        this.setState({
          materi: resdata.DATA,
          refreshing: false,
        });
      });
  }

  GetTutorAPi() {
    fetch('https://kilauindonesia.org/datakilau/api/testo')
      .then(res => {
        if (res.status === 200) return res.json();
      })
      .then(resdata => {
        console.log(resdata.data);
        this.setState({
          Tutor: resdata.data,
          filter: resdata.DATA,
          refreshing: false,
        });
      });
  }
